{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "scripts/(.*)", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)\\.(css|js|png|jpg|svg|json|ico|pdf)", "dest": "/$1.$2" },
    { "src": "/", "dest": "/index.html" }
  ]
}
