{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "style.css", "use": "@vercel/static" },
    { "src": "scripts/core.js", "use": "@vercel/static" },
    { "src": "scripts/semantic.js", "use": "@vercel/static" },
    { "src": "scripts/concursos.js", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/scripts/(.*)",
      "dest": "/scripts/$1",
      "headers": {
        "Content-Type": "application/javascript; charset=utf-8",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/style\\.css",
      "dest": "/style.css",
      "headers": {
        "Content-Type": "text/css; charset=utf-8",
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/favicon\\.ico",
      "dest": "/favicon.ico",
      "headers": {
        "Content-Type": "image/x-icon"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}
